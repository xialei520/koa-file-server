{
    "name": "koa-file-server",
    "version": "1.0.0",
    "description": "一个用于存储文件的服务器",
    "main": "server.ts",
    "scripts": {
        "start": "tsnd --respawn server.ts",
        "cz": "git cz",
        "genlog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0"
    },
    "config": {
        "commitizen": {
            "path": "node_modules/cz-conventional-changelog"
        }
    },
    "keywords": [
        "koa",
        "server",
        "file"
    ],
    "author": "xialei",
    "license": "ISC",
    "devDependencies": {
        "@types/koa": "^2.13.5",
        "@types/koa-conditional-get": "^2.0.0",
        "@types/koa-etag": "^3.0.0",
        "@types/koa-static": "^4.0.2",
        "@types/koa-static-cache": "^5.1.1",
        "commitizen": "^4.2.5",
        "conventional-changelog-cli": "^2.2.2",
        "cz-conventional-changelog": "^3.3.0",
        "koa-conditional-get": "^3.0.0",
        "koa-etag": "^4.0.0",
        "ts-node-dev": "^2.0.0",
        "typescript": "^4.8.4"
    },
    "dependencies": {
        "koa": "^2.13.4",
        "koa-static": "^5.0.0",
        "koa-static-cache": "^5.1.4"
    }
}
